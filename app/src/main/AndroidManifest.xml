<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kukuhsain.simpletour.admin">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.kukuhsain.simpletour.admin.SimpleTourApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">
        <activity android:name="com.kukuhsain.simpletour.admin.view.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.kukuhsain.simpletour.admin.view.SignInActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.kukuhsain.simpletour.admin.view.SignUpActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.kukuhsain.simpletour.admin.view.DestinationsActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.kukuhsain.simpletour.admin.view.PackagesActivity"
            android:parentActivityName="com.kukuhsain.simpletour.admin.view.DestinationsActivity"
            android:screenOrientation="portrait">
            <!--<meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value=".view.DestinationsActivity"/>-->
        </activity>
    </application>

</manifest>